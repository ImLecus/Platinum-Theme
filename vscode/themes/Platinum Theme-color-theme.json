		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown",
				"meta.paragraph.markdown"
			],
			"settings": {
				"foreground": "#d2d4d5"
			}
		},
		{
			"name": "Markdown - Scape",
			"scope": [
				"punctuation.definition.raw.markdown",
